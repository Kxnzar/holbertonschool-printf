#include "main.h"
#include <stdio.h>
#include <stlib.h>
#include <stdarg.h>

/**
* _printf - Produce the output according to a format.
* @format: A character string.
* @...: A variable
*
* Return: The number of characters printed.
*/

int _printf(const char *format, ...)
{
  va_list prints;
  int p;

  va_start(prints, format);
  p = (stdout, format, prints);
  va _end(prints);

  return(p);
}
